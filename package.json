{
  "engines": {
    "node": ">=18.x.x",
    "npm": ">=8.x.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/main.ts\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/config": "^3.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/platform-socket.io": "^10.2.4",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/websockets": "^10.2.4",
    "@sendgrid/mail": "^7.7.0",
    "@types/mailgun-js": "^0.22.13",
    "@types/passport-jwt": "^3.0.9",
    "@types/sequelize": "^4.28.15",
    "archiver": "^7.0.1",
    "async-retry": "^1.3.3",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.2.3",
    "cache-manager-memcached-store": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "docxtemplater": "^3.37.14",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "exceljs": "^4.3.0",
    "express-session": "^1.17.3",
